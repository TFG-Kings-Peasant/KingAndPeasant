
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model User {
  idUser  Int      @id @default(autoincrement())
  name    String   @unique
  email   String   @unique
  password  String
  games   Int      @default(0)
  wins    Int      @default(0)
  losses  Int      @default(0)
  //ProfilePicture  String 
}